    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "pbkdf2": "^3.0.17",
    "react-dom": "^16.12.0",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/src/tests/jestSetup.js"
    ],
    "moduleNameMapper": {
      "Server": "<rootDir>/src/tests/server",
      "Db": "<rootDir>/src/tests/mocks/db",
      "^I18n": "<rootDir>/src/tests/mocks/i18n",
      "^react-native-gesture-handler$": "<rootDir>/src/tests/mocks/react-native-gesture-handler"
    },
    "globals": {
      "window": {}
    },
    "transformIgnorePatterns": [
      "node_modules/?!(react-native|native-base)"
    ]
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "http": "@tradle/react-native-http",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "http": "@tradle/react-native-http",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  }
