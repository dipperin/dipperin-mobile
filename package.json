    "@react-native-community/viewpager": "^3.3.0",
    "@tradle/react-native-http": "^2.0.1",
    "@types/aes-js": "^3.1.0",
    "@types/enzyme": "^3.10.4",
    "aes-js": "^3.1.2",
    "assert": "^2.0.0",
    "asyncstorage-down": "^4.2.0",
    "bignumber.js": "^9.0.0",
    "bip39": "2.4.0",
    "buffer": "^4.9.2",
    "date-fns": "^2.9.0",
